{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/api/*",
      "serve": "/api/index.js",
      "statusCode": 200
    },
    {
      "route": "/{*}",
      "serve": "/index.html",
      "statusCode": 404
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*"]
  }
}